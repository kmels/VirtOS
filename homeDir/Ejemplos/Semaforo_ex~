; reiniciar todos los registros menos el 0 (ya que es el contador)
Set r0 3

wait r0

echo "bla0\n"
echo "bla1\n"
echo "bla2\n"
echo "bla3\n"
echo "bla4\n"
echo "bla5\n"
echo "bla6\n"
echo "bla7\n"
echo "bla8\n"
echo "bla9\n"
echo "bla10\n"

signal r0

end 1
