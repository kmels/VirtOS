;###############################################
; Saltos
;###############################################


set r0 3
'salto_1' echo r0 ;Imprime lo que tiene el registro 0
set r1 1
sub r0 r0 r1
jmp r0 salto_1

echo " "

set r0 5
'salto_2' echo r0 ;Imprime lo que tiene el registro 0
set r1 1
sub r0 r0 r1
jmp r0 salto_2

end 0

;tiene que imprimir "321 54321"
